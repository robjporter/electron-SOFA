{
  "name": "sofa",
  "productName": "SOFA",
  "version": "0.0.1",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "private": true,
  "keywords": [],
  "bugs": {
    "url": "https://github.com/robjporter/electron-SOFA/issues"
  },
  "author": {
    "name": "Rob Porter",
    "email": "rjpdesigns@gmail.com",
    "url": "https://github.com/robjporter"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/robjporter/electron-SOFA.git"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",

    "publish2": "build -p always"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/images/icon/icon.png.icns"
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "./src/images/icon/icon.png.icns"
      },
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm",
          "flatpak",
          "snap"
        ]
      },
      "electronWinstallerConfig": {
        "name": "ng-example"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "robjporter",
        "name": "electron-SOFA"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "ngExample"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_webpack"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux",
            "windows"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack/webpack.main.config.js",
            "renderer": {
              "config": "./webpack/webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/renderer/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "robjporter",
              "name": "electron-"
            },
            "prerelease": true
          }
        }
      ]
    }
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "bootstrap": "^4.3.1",
    "electron-log": "latest",
    "electron-squirrel-startup": "^1.0.0",
    "electron-unhandled": "latest",
    "electron-util": "latest",
    "file-loader": "^4.0.0",
    "react": "latest",
    "react-dom": "latest",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "reactstrap": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@electron-forge/cli": "latest",
    "@electron-forge/maker-deb": "latest",
    "@electron-forge/maker-rpm": "latest",
    "@electron-forge/maker-squirrel": "latest",
    "@electron-forge/maker-zip": "latest",
    "@electron-forge/plugin-webpack": "latest",
    "@electron-forge/publisher-github": "latest",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "latest",
    "electron": "latest",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "latest",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.5.0",
    "node-loader": "latest",
    "react-hot-loader": "latest",
    "style-loader": "latest",
    "ts-loader": "latest",
    "typescript": "latest"
  },
  "build": {
    "appId": "com.roporter.SOFA",
    "productName": "SOFA",
    "publish": [
      {
        "provider": "github",
        "owner": "robjporter",
        "repo": "electron-SOFA"
      }
    ],
    "mac": {
      "category": "public.app-category.social-networking",
      "darkModeSupport": true,
      "gatekeeperAssess": false
    }
  }
}
